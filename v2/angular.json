{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "options": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/options",
      "sourceRoot": "projects/options/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/options",
            "index": "projects/options/src/options.html",
            "browser": "projects/options/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/options/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/options/public"
              }
            ],
            "styles": [
              "projects/options/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4200
          },
          "configurations": {
            "production": {
              "buildTarget": "options:build:production"
            },
            "development": {
              "buildTarget": "options:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/options/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/options/public"
              }
            ],
            "styles": [
              "projects/options/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "main": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/main",
      "sourceRoot": "projects/main/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/main",
            "index": "projects/main/src/index.html",
            "browser": "projects/main/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/main/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/main/public"
              },
              {
                "glob": "**/*",
                "input": "projects/main/src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/main/src/styles.css"
            ],
            "scripts": [],
            "crossOrigin": "anonymous",
            "inlineStyleLanguage": "css"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4201
          },
          "configurations": {
            "production": {
              "buildTarget": "main:build:production"
            },
            "development": {
              "buildTarget": "main:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/main/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/main/public"
              },
              {
                "glob": "**/*",
                "input": "projects/main/src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/main/src/styles.css"
            ],
            "scripts": []
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/main/.storybook",
            "browserTarget": "main:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/main"
            ],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/main/.storybook",
            "browserTarget": "main:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/main"
            ],
            "outputDir": "dist/storybook/main"
          }
        }
      }
    }
  }
}
